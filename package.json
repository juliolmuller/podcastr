    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
